#include<stdio.h>
#include<string.h>
int main(){
	char str[100];
	printf("enter a string:");
	scanf("%s",str);
	int l=0;
	int h=strlen(str)-1;
	while(h>l){
		if(str[l++]!=str[h--]){
			printf("%s,it is a palindrome:",str);
			return 0;
		}
	}
	
	printf("%s,it is not a palindrome:",str);
	return 0;
}
